{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["./WebAPI"],
        "exclude": [
            "./WebAPI/VRGlobe.js",
            "./WebAPI/ThirdParty/"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "opts": {
        "template": "./templates/jaguarjs",
        "encoding": "utf8",
        "destination": "./Build/Documentation",
        "recurse": true
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles": true
        },
        "applicationName": "VRGlobe",
        "disqus": "",
        "googleAnalytics": "",
        "openGraph": {
            "title": "",
            "type": "website",
            "image": "",
            "site_name": "",
            "url": ""
        },
        "meta": {
            "title": "",
            "description": "",
            "keyword": ""
        },
        "linenums": false
    }
}